                "--no-sound-null-safety",
                "--cache-sksl",
                "--purge-persistent-cache",
            ]
        },
        {
            "name": "Run Release Prod",
            "type": "dart",
            "request": "launch",
            "flutterMode": "release",
            "program": "lib/app/flavors/main_production.dart",
            "args": [
                "--no-sound-null-safety",
                "--cache-sksl",
                "--purge-persistent-cache",
            ]
        },
        {
            "name": "Run Debug Dev",
            "type": "dart",
            "request": "launch",
