    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Stage",
            "type": "dart",
            "request": "launch",
            "flutterMode": "debug",
            "program": "lib/app/flavors/main_staging.dart",
            "args": [
                "--no-sound-null-safety",
                "--cache-sksl",
                "--purge-persistent-cache",
            ]
        },
        {
            "name": "Run Debug Prod",
            "type": "dart",
            "request": "launch",
            "flutterMode": "debug",
